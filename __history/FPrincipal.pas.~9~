unit FPrincipal;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.ComCtrls, Vcl.Menus, Vcl.ExtCtrls,
  Vcl.ActnMan, Vcl.ActnCtrls, Vcl.ActnMenus, Vcl.ToolWin;

type
  Tprincipal = class(TForm)
    StatusBar1: TStatusBar;
    MainMenuPrincipal: TMainMenu;
    ddd1: TMenuItem;
    Provedores1: TMenuItem;
    Administradores1: TMenuItem;
    rabajadores1: TMenuItem;
    PageControl1: TPageControl;
    Panel1: TPanel;
    Panel2: TPanel;
    PageControl2: TPageControl;
    Splitter1: TSplitter;
    procedure FormCreate(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  principal: Tprincipal;

implementation

{$R *.dfm}

uses BOTONES;

procedure Tprincipal.FormCreate(Sender: TObject);
var botons:Tmenuseleccion;
begin
    botons:=Tmenuseleccion.Create(principal);
    botons.Show;
    pagecontrol1.ManualDock(botons);
end;

end.
